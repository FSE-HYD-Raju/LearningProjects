{
    "optional": [
        "runtime"
    ],
    "stage": 0,
    "env" : {
        "development": {
            "env": {
                "browser": {
                    "plugins": [
                        "react-transform"
                    ],
                    "extra": {
                        "react-transform": {
                            "transforms": [
                                {
                                    "transform": "react-transform-hmr",
                                    "imports": [
                                        "react"
                                    ],
                                    "locals": [
                                        "module"
                                    ]
                                },
                                {
                                    "transform": "react-transform-catch-errors",
                                    "imports": [
                                        "react",
                                        "redbox-react"
                                    ]
                                }
                            ]
                        }
                    }
                }
            }
        }
    }
}
